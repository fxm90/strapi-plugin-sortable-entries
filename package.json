{
  "name": "strapi-plugin-sortable-entries",
  "version": "1.2.0-beta.0",
  "description": "A Strapi plugin that enables drag-and-drop sorting of entries within a collection type.",
  "keywords": [
    "strapi",
    "plugin",
    "sortable",
    "entries",
    "drag",
    "drop",
    "order",
    "sort"
  ],
  "license": "MIT",
  "author": "Felix M. <me@felix.hamburg>",
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "test": "vitest",
    "test:ts:back": "run -T tsc -p server/tsconfig.json",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "verify": "strapi-plugin verify",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@strapi/design-system": "^2.0.0-rc.28",
    "@strapi/icons": "^2.0.0-rc.28",
    "react-intl": "^7.1.11"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.24.1",
    "@strapi/typescript-utils": "^5.17.0",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.18.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  },
  "strapi": {
    "kind": "plugin",
    "name": "sortable-entries",
    "displayName": "Sortable Entries",
    "description": "A Strapi plugin that enables drag-and-drop sorting of entries within a collection type."
  }
}
